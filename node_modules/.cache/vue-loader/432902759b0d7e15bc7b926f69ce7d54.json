it-line-clamp: $line-clamp;\r\n}\r\n@mixin logo-display($logoSize: 36px, $logoRadius: 36px) {\r\n  height: $logoSize;\r\n  width: $logoSize;\r\n  -webkit-border-radius: $logoRadius;\r\n  border-radius: $logoRadius;\r\n  margin-right: 12px;\r\n  flex-shrink: 0;\r\n}\r\n.card {\r\n  >>> .el-card__body {\r\n    padding: 0;\r\n  }\r\n  &-item {\r\n    margin-bottom: 16px;\r\n    height: 116px;\r\n    font-size: 10px;\r\n    line-height: 20px;\r\n    background-color: #fff;\r\n    box-sizing: border-box;\r\n    a {\r\n      text-decoration: none;\r\n    }\r\n  }\r\n  &-body {\r\n    display: flex;\r\n    padding: 10px;\r\n    &-logo {\r\n      @include logo-display;\r\n      margin-top: 15px;\r\n    }\r\n    &-largelogo {\r\n      @include logo-display(96px, 3px);\r\n    }\r\n    &-detail {\r\n      flex: 1;\r\n      &-title {\r\n        @include text-display;\r\n        font-size: 14px;\r\n        color: rgba(0, 0, 0, 0.85);\r\n        margin-bottom: 6px;\r\n      }\r\n      &-content {\r\n        @include text-display(2);\r\n        height: 40px;\r\n        color: rgba(0, 0, 0, 0.45);\r\n      }\r\n      &-similarity {\r\n        margin-top: 6px;\r\n        display: flex;\r\n        justify-content: space-between;\r\n        color: rgba(0, 0, 0, 0.45);\r\n        i {\r\n          font-size: 20px;\r\n        }\r\n      }\r\n    }\r\n  }\r\n  &-menu {\r\n    display: flex;\r\n    justify-content: space-around;\r\n    height: 28px;\r\n    background: #f7f9fa;\r\n    &-author {\r\n      padding-top: 4px;\r\n    }\r\n    &-icon {\r\n      display: flex;\r\n      align-items: center;\r\n      i {\r\n        font-size: 18px;\r\n      }\r\n      p {\r\n        margin-left: 5px;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}